{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "only-arrow-functions": false,
    "no-unused-expression": false,
    "no-debugger": false,
    "no-trailing-whitespace": false,
    "quotemark": false,
    "whitespace": false,
    "max-line-length": false
  }
}
